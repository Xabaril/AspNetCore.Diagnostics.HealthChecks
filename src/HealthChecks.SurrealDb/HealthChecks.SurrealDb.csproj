<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <PackageTags>$(PackageTags);SurrealDb</PackageTags>
    <Description>HealthChecks.SurrealDb is the health check package for SurrealDB.</Description>
    <VersionPrefix>$(HealthCheckSurrealDb)</VersionPrefix>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Diagnostics.HealthChecks" Version="8.0.0-rc.1.23421.29" />
    <PackageReference Include="SurrealDb.Net" Version="0.2.0" />
  </ItemGroup>

</Project>
